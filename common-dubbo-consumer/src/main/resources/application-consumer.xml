<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">
    <dubbo:application name="app-consumer" />

    <dubbo:registry  protocol="zookeeper" address="localhost:2181" />

    <!-- Demo Service -->
    <dubbo:reference interface="org.wsh.common.consumer.service.DemoService" id="demoService" version="1.0.0"/>
    <dubbo:reference interface="org.wsh.common.consumer.service.DemoAService" id="demoAService" version="1.0.0"/>
    <dubbo:reference interface="org.wsh.common.consumer.service.DemoBService" id="demoBService" version="1.0.0"/>

    <!-- Basic Service -->
    <dubbo:reference interface="org.wsh.common.service.api.MenuService" id="menuService" version="1.0.0"/>
    <dubbo:reference interface="org.wsh.common.service.api.UserService" id="userService" version="1.0.0"/>
    <dubbo:reference interface="org.wsh.common.service.api.RoleService" id="roleService" version="1.0.0"/>
    <dubbo:reference interface="org.wsh.common.service.api.PermissionService" id="permissionService" version="1.0.0"/>
    <dubbo:reference interface="org.wsh.common.service.api.ModuleService" id="moduleService" version="1.0.0"/>

</beans>