<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">
    <dubbo:application name="app-consumer" />

    <dubbo:registry  protocol="zookeeper" address="121.43.116.29:2181" />
    <!-- 要引用的服务 -->
    <dubbo:reference interface="com.xxx.consumer.service.DemoService" id="demoService"/>

    <dubbo:reference interface="com.xxx.consumer.service.DemoAService" id="demoAService"/>

    <dubbo:reference interface="com.xxx.consumer.service.DemoBService" id="demoBService"/>
</beans>